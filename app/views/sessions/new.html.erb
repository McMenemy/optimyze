<div class="app">
  <div class="left-pane">
    <form class="user-form" action="<%= session_url %>" method="post">
      <%= auth_token_input %>

      <div class="formGroup">
        <label>Username</label>
        <input type="text" name="user[username]" value="">
      </div>

      <div class="formGroup">
        <label>Password</label>
        <input type="password" name="user[password]" value="">
      </div>

      <input class="whiteButton" type="submit" value="Sign In">
    </form>
  </div>
</div>

